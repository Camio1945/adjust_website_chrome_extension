{
  "name": "A调整网站",
  "description": "A调整网站",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://blog.csdn.net/*",
        "https://stackoverflow.com/*",
        "https://www.jianshu.com/*",
        "https://www.zhihu.com/*",
        "https://www.cnblogs.com/*",
        "https://coolshell.cn/*",
        "https://www.processon.com/*"
      ],
      "js": [
        "jquery-1.11.0.min.js",
        "adjust.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "A调整页面"
  },
  "manifest_version": 2
}
